{
    "manifest_version": 2,
    "name": "chrome locker",
    "description": "Locks the chrome and ask for password everytime you open chrome",
	"version": "1.0.0",
	"commands": {
		"toggle": {
			"suggested_key": {
				"default": "Ctrl+Shift+L",
				"mac": "Command+Shift+L"
			},
		"description": "Locks the screen",
		"global": false
		}
	},
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"icons": {
		"16": "ico16.png",
		"48": "ico48.png",
		"64": "ico64.png",
		"128": "ico128.png"
	},
	"browser_action": {
		"default_icon": {
			"16": "ico16.png",
			"48": "ico48.png",
			"64": "ico64.png",
			"128": "ico128.png"	
		}
	}
}
